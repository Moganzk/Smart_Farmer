{
  "name": "smart-farmer",
  "version": "1.0.0",
  "description": "Offline-first crop disease detection app",
  "private": true,
  "workspaces": [
    "smart_farmer",
    "smart_farmer.tests"
  ],
  "scripts": {
    "test": "cd smart_farmer.tests && npm run test:unit",
    "test:unit": "cd smart_farmer.tests && npm run test:unit",
    "test:integration": "cd smart_farmer.tests && npm run test:integration",
    "test:all": "cd smart_farmer.tests && npm run test:all",
    "test:watch": "cd smart_farmer.tests && npm run test:watch",
    "test:coverage": "cd smart_farmer.tests && npm run test:coverage",
    "test:install": "cd smart_farmer.tests && npm install",
    "android:build:preview": "npx eas-cli build --profile preview --platform android",
    "android:build:production": "npx eas-cli build --profile production --platform android",
    "android:emulator:list": "emulator -list-avds",
    "android:emulator:start": "emulator @Pixel_6_API_34 -no-snapshot-load",
    "android:adb:devices": "adb devices",
    "android:apk:install": "adb install -r",
    "expo:doctor": "npx expo-doctor",
    "expo:prebuild": "npx expo prebuild --no-install",
    "ios:build": "npx eas-cli build --profile development --platform ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Moganzk/Smart_Farmer.git"
  },
  "license": "MIT",
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/stack": "^7.6.16",
    "@supabase/supabase-js": "^2.93.3",
    "expo": "^54.0.33",
    "expo-camera": "^17.0.10",
    "expo-dev-client": "^6.0.20",
    "expo-file-system": "^19.0.21",
    "expo-sqlite": "~16.0.10",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10"
  }
}
