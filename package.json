{
  "name": "smart-farmer",
  "version": "1.0.0",
  "description": "Offline-first crop disease detection app",
  "scripts": {
    "test": "cd smart_farmer.tests && npm run test:unit",
    "test:unit": "cd smart_farmer.tests && npm run test:unit",
    "test:integration": "cd smart_farmer.tests && npm run test:integration",
    "test:all": "cd smart_farmer.tests && npm run test:all",
    "test:watch": "cd smart_farmer.tests && npm run test:watch",
    "test:coverage": "cd smart_farmer.tests && npm run test:coverage",
    "test:install": "cd smart_farmer.tests && npm install",
    "android:build": "cd smart_farmer && eas build --profile development --platform android",
    "ios:build": "cd smart_farmer && eas build --profile development --platform ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Moganzk/Smart_Farmer.git"
  },
  "license": "MIT",
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/stack": "^7.6.16",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.20.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.9",
    "@types/react-native": "^0.72.8"
  }
}
